from bs4 import BeautifulSoup
import requests

URL = "https://www.instagram.com/amelia_janek_/"
request = request.get(URL)
s = BeutifulSoup(request.text, "html.parser")
meta = s.find("meta", property = "og:description")
s = meta.attrs['content'].split(" ")
data = {}
data['Followers'] = s[0]
data['Following'] = s[2]
data['Posts'] = s[4]
print(data)
